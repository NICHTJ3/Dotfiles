{
    "description": "My dotnet helper scripts",
    "links": [
        {
            "from": "./config/dotnet/helper-scripts/*",
            "to": "~/bin/helper-scripts",
            "glob": true
        }
    ],
    "shell": [
        "dotnet-install.sh"
    ],
    "revert_shell": ["rm -r ~/.dotnet"]
}
