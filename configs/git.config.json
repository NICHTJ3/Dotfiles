{
  "links": [
    {
      "to": "~/bin/helper-scripts",
      "from": "./configs/git/config/helper-scripts/*",
      "glob": true
    },
    {
      "to": "~/.config/git",
      "from": "./configs/git/config/git/*",
      "glob": true
    }
  ],
  "packages": [
    "git",
    "lazygit",
    "gh"
  ]
}
