{
  "npm.binPath": "yarn",
  "prettier.eslintIntegration": true,
  "prettier.disableLanguages": [],
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all",
  "eslint.quiet": true,
  "eslint.packageManager": "yarn",
  "markdownlint.onChange": false,
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": " ", // TODO: Should this sign be different than the one below
  "diagnostic.hintSign": " ",
  "snippets.priority": 100,
  "coc.source.word.filetypes": ["markdown", "org"],
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "python.workspaceSymbols.tagFilePath": "${workspaceFolder}/tags",
  "python.linting.ignorePatterns": ["*-test.py"],
  "python.venvPath": "/home/nichtj3/.local/share/virtualenvs/",
  "javascript.validate.enable": false,
  "python.formatting.provider": "black",
  "svelte.plugin.typescript.diagnostics.enable": false,
  "xml.format.spaceBeforeEmptyCloseTag": true,
  "xml.format.splitAttributes": true,
  "xml.completion.autoCloseTags": true,
  "xml.validation.enabled": false,
  "explorer.position": "right",
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.previewAction.onHover": false,
  "explorer.width": 30,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "explorer.presets": {
    "simplify": {
      "file-child-template": "[selection | clip | 1] [indent][icon | 1] [filename omitCenter 1]"
    }
  },
  "clangd.semanticHighlighting": true,
  "rust-analyzer.lens.run": false,
  "tailwindCSS.headwind.runOnSave": false,
  "svelte.plugin.css.diagnostics.enable": false,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      // customize filetypes to your needs
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    }
  },
  "Lua.diagnostics.globals":["vim","use"]
}
