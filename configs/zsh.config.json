{
  "description": "My zsh config",
  "links": [
    {
      "from": "zsh/config/helper-scripts/*",
      "to": "~/bin/helper-scripts/",
      "glob": true
    },
    {
      "from": "zsh/config/zsh/zshenv",
      "to": "~/.zshenv"
    },
    {
      "from": "zsh/config/zsh/zshrc",
      "to": "~/.zshrc"
    },
    {
      "from": "zsh/config/zsh/p10k.zsh",
      "to": "~/.config/zsh/p10k.zsh"
    },
    {
      "from": "zsh/config/zsh/p10k-portable.zsh",
      "to": "~/.config/zsh/p10k-portable.zsh"
    }
  ],
  "shell": ["touch ~/.z", "pnpm install-completion zsh"]
}
